<configs>

	<append xpath="/Sounds">
		<SoundDataNode name="Bobcat_horn">
      <AudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab"/>
      <NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire_Network.prefab"/>
      <!-- <AudioSource name="Sounds/AudioSource_WeaponFire"/>
      <NetworkAudioSource name="Sounds/AudioSource_WeaponFire_Network"/> -->
			<Noise ID="5" noise="200" time="2" heat_map_strength="0" heat_map_time="0"/>
			<AudioClip ClipName="#@modfolder:Resources/BobcatHornPrefab.unity3d?BobcatHorn"/>
			<LocalCrouchVolumeScale value="1"/>
			<CrouchNoiseScale value="1"/>
			<NoiseScale value="1"/>
			<MaxVoices value="20"/>
			<MaxVoicesPerEntity value="6"/>
			<MaxRepeatRate value="0.001"/>
		</SoundDataNode>

    <SoundDataNode name="Bobcat_Silence">
      <AudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab"/>
      <NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire_Network.prefab"/>
	<!-- <AudioSource name="Sounds/AudioSource_WeaponFire"/>
      <NetworkAudioSource name="Sounds/AudioSource_WeaponFire_Network"/> -->
			<Noise ID="5" noise="200" time="2" heat_map_strength="0" heat_map_time="0"/>
			<AudioClip ClipName="#@modfolder:Resources/SilencePrefab.unity3d?Silence"/>
			<LocalCrouchVolumeScale value="1"/>
			<CrouchNoiseScale value="1"/>
			<NoiseScale value="1"/>
			<MaxVoices value="20"/>
			<MaxVoicesPerEntity value="6"/>
			<MaxRepeatRate value="0.001"/>
		</SoundDataNode>
	</append>
</configs>